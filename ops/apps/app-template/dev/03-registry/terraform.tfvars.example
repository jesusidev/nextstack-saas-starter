# AWS Configuration
region  = "us-west-2"
profile = "default"

# Repository Configuration
repository_name      = "app-template-dev"
image_tag_mutability = "MUTABLE"
scan_on_push         = true

# Lifecycle Configuration
image_retention_count    = 10
untagged_retention_days  = 7

# Tags
tags = {
  Project     = "AppTemplate"
  Environment = "dev"
  ManagedBy   = "terraform"
  Team        = "engineering"
}
